{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Model",
  "description": "A Model to use if the main model does not return a response",
  "properties": {
    "name": {
      "type": "string",
      "title": "Name",
      "description": "Name of the model"
    },
    "version": {
      "type": "string",
      "title": "Version",
      "description": "Version of the model"
    },
    "developer": {
      "type": "string",
      "title": "Developer",
      "description": "Developer of the model"
    },
    "host": {
      "type": "string",
      "title": "Host",
      "description": "Host of the model, cloud provider, or ai company"
    }
  },
  "required": [
    "name",
    "version",
    "provider"
  ]
}
